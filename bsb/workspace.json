{
  "version": 2,
  "projects": {
    "validate": {
      "root": "libs/validate",
      "sourceRoot": "libs/validate/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/validate/**/*.{ts,tsx,js,jsx}"
            ]
          }
        },
        "build": {
          "executor": "@nrwl/web:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/validate",
            "tsConfig": "libs/validate/tsconfig.lib.json",
            "project": "libs/validate/package.json",
            "entryFile": "libs/validate/src/index.ts",
            "external": [
              "react",
              "react-dom"
            ],
            "babelConfig": "@nrwl/react/plugins/bundle-babel",
            "rollupConfig": "@nrwl/react/plugins/bundle-rollup",
            "assets": [
              {
                "glob": "README.md",
                "input": ".",
                "output": "."
              }
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/validate"
          ],
          "options": {
            "jestConfig": "libs/validate/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/workspace"
  }
}